{
  "name": "@cap402/sdk",
  "version": "0.1.0",
  "description": "CAP-402 SDK - Privacy-first agent infrastructure for autonomous AI agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./agent": {
      "require": "./dist/agent.js",
      "import": "./dist/agent.mjs",
      "types": "./dist/agent.d.ts"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "cap402",
    "agent",
    "ai-agent",
    "autonomous-agent",
    "privacy",
    "zk-proofs",
    "confidential-compute",
    "solana",
    "arcium",
    "noir",
    "fhe",
    "capability-routing",
    "agent-infrastructure",
    "mcp",
    "a2a-protocol"
  ],
  "author": "CAP-402 Team <hello@intym.xyz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cap402/cap402.git"
  },
  "homepage": "https://cap402.com",
  "bugs": {
    "url": "https://github.com/cap402/cap402/issues"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.87.0"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
